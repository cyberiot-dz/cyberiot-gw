    "name": "euedge-influxdb",
    "categories": ["database"],
    "platform": "linux",
    "logo": "http://192.168.31.245/dz/icon_influxdb.svg",
    "image": "euedge/docker-euedge-influxdb:1.1.0",
    "hostname": "euedge-influxdb",
    "network": "cyberiot_euedge-network",
    "ports": [
      "8083:8083/tcp",
      "8086:8086/tcp"
