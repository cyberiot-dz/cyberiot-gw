    "volumes": [{"container": "/var/lib/influxdb"}],
    "env": [
      {
        "name": "INFLUXDB_ADMIN_ENABLED",
        "default": "true",
        "preset": true
      },
      {
        "name": "INFLUXDB_DB",
        "default": "\"cyberiot-influxdb\"",
        "preset": true
      }
    ]
  },
